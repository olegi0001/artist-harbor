{
	"env": {
		"browser": true,
		"es2021": true,
		"jest": true,
		"node": true,
		"jest/globals": true
	},
	"extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "plugin:jest/recommended"],
	"parser": "@typescript-eslint/parser",
	"plugins": ["react", "jest"],
	"rules": {
		"no-unused-vars": "off",
		"@typescript-eslint/no-unused-vars": ["warn"]
	}
}
